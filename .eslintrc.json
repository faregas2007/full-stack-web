{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": [0],
    "import/extensions": "off"
  },
  "env": {
    "es6":true,
    "browser": true,
    "node": true,
    "jest": true,
  }
}
